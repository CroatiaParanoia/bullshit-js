#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/dist/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules:/Users/croatialu/Documents/personal/node_modules:/Users/croatialu/Documents/node_modules:/Users/croatialu/node_modules:/Users/node_modules:/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/ts-node/dist/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/ts-node/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/dist/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules/ts-node/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/ts-node@10.5.0_typescript@4.5.5/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/.pnpm/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules:/Users/croatialu/Documents/personal/node_modules:/Users/croatialu/Documents/node_modules:/Users/croatialu/node_modules:/Users/node_modules:/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/ts-node/dist/node_modules:/Users/croatialu/Documents/personal/bullshit-js/node_modules/ts-node/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi
